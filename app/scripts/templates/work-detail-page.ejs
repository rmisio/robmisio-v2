<div class="images">
    <% 
      for (var i=0; i < images.length; i++) { 
    %>
        <img class="photo" src="/images/work/<%= images[i] %>" />
    <% } %>
</div>

<div class="content">
  <h1><%= title %></h1>
  <ul class="tags list-unstyled">
    <% 
      for (var j=0; j < tags.length; j++) { 
    %>
      <li><%= tags[j] %></li>
    <% } %>
  </ul>
  <%= description %>  
  <a class="btn" href="<%= liveUrl %>" target="_blank">View Website</a>
</div>