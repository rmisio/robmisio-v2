<div class="album">
<% for (var i=0; i < photos.length; i++) {
    var p = photos[i],
        height = p.cloudinary.data.height,
        width = p.cloudinary.data.width;
    %>

    <div class="defer-image photo"
        data-photo-height="<%= height %>"
        data-photo-width="<%= width %>">
        <div data-class="img-responsive"
            data-src="<%= clUrl(p.cloudinary.id + '.' + p.cloudinary.data.format, { height: 675, width: deviceWidth(), crop: 'fit'}) %>"
            style="padding-top: <%= height / width * 100 %>%">
        </div>
    </div>

<% } %>
</div>
