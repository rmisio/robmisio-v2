<% if (uploadCount) { %>
<p>Uploading <%= uploadCount %> files</p>
<% } else { %>
<p>Uploads complete.</p>
<% } %>

<div class="progress">
  <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= uploadPercent %>%;">
    <%= uploadPercent %>%
  </div>
</div>
